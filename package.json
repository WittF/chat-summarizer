{
  "name": "koishi-plugin-chat-summarizer",
  "description": "Koishi聊天记录收集和上传插件",
  "version": "0.0.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "keywords": [
    "koishi",
    "plugin",
    "chat"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=lib/index.js --format=cjs --platform=node --external:koishi --external:@aws-sdk/client-s3 --external:@aws-sdk/lib-storage --external:axios && tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/lib-storage": "^3.658.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {
    "koishi": "4.18.7"
  }
}
